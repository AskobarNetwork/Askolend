oryI.sol\":{\"keccak256\":\"0x981c9357af404e55fe0fd912e7e7c73d647f5bb1cce7a2949993f9df20f09f6f\",\"urls\":[\"bzz-raw://a5635d1ed254a5fde290a24ed95488e8db82f7c6d02c2f4c4b77e86f18019913\",\"dweb:/ipfs/Qmdosfmt54V9utA1nsShrQAGGHoKffzVLpPP9ZxV1YGe74\"]}},\"version\":1}",
